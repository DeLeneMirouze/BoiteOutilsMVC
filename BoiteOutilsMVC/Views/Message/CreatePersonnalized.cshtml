@model BoiteOutilsMVC.Domaine.CompteClient

@{
    ViewBag.Title = "CreatePersonnalized";
}

<h2>CreatePersonnalized</h2>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>CompteClient</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.Logon)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Logon)
            @Html.ValidationMessageFor(model => model.Logon)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.MotDePasse)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.MotDePasse)
            @Html.ValidationMessageFor(model => model.MotDePasse)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.MotDePasse2)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.MotDePasse2)
            @Html.ValidationMessageFor(model => model.MotDePasse2)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.Name)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Name)
            @Html.ValidationMessageFor(model => model.Name)
        </div>

        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
